{
	"from": 0,
	"size": 100000,
	"query": {
		"bool": {
			"filter": [{
				"type": {
					"value": "SmartDeviceDoorStatus"
				}
			}, {
				"range": {
					"DoorCount": {
						"gte": 0
					}
				}
			}, {
				"term": {
					"IsDeleted": false
				}
			}],
			"must_not": [{
				"term": {
					"AssetId": 0
				}
			}]
		}
	},
	"aggs": {
		"Door": {
			"date_histogram": {
				"field": "EventTime",
				"interval": "month",
				"min_doc_count": 1
			},
			"aggs": {
				"DoorCount": {
					"sum": {
						"field": "DoorCount"
					}
				},
				"Asset": {
					"terms": {
						"field": "AssetId",
						"size": 100000
					},
					"aggs": {
						"DoorCount": {
							"sum": {
								"field": "DoorCount"
							}
						}
					}
				}
			}
		}
	}
}